				<list>
					<value>shankar1@gmail.com</value>
					<value>shankar2@gmail.com</value>
					<null />
				</list>
		</property>
		<property name="teacherPhones">
			<set>
				<value>234662</value>
				<value>234555</value>
			</set>
		</property>
		<property name="teacherExperience">
			<map>
				<entry key="DAV" value="6"/>
				<entry key="KV" value="3"/>
			</map>
		</property>
		
	</bean>
        
</beans>